{
    "manifest_version": 2,
    "name": "URL Blocker Extension - Productivity App",
    "version": "0.0.1",
    "description": "A URL Blocker Extension",
    "permissions": ["storage", "activeTab", "<all_urls>", "webRequest", "webRequestBlocking", "webNavigation"],
    "browser_action":{
        "default_popup": "index.html"
    },
    "background":{
        "scripts":["script/script.js"]
    }

}